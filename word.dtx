% \begin{macro}{\if西暦}
% \begin{macro}{\西暦}
% \begin{macro}{\和暦}
% \changes{v1.0h}{1996/12/17}{Typo:和歴 to 和暦}
% |\today|コマンドの`年'を、
% 西暦か和暦のどちらで出力するかを指定するコマンドです。
%    \begin{macrocode}
\newif\if西暦 \西暦false
\def\西暦{\西暦true}
